{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "removeComments": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false,
    "suppressImplicitAnyIndexErrors": true,
    "incremental": true,
    "paths": {
       "@/*": ["./*"],
      "@/public/*": [
        "./public/*"
      ]
    }
  },
  // "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "**/*.jsx","**/*.js", "src/server.js", "src/server/server.js", "src/pages/index.jsx", "src/lib/services/emailService.js"],
  "exclude": [
    "node_modules"
  ],
  
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
